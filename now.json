{
  "name": "triangulo-dev",
  "alias": "triangulo.dev",
  "version": 2,
  "routes": [
    {
      "src": "^/dist/css/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "^/images/(.*)",
      "headers": { "cache-control": "max-age=31536000,immutable" }
    },
    {
      "src": "^/(.*).html",
      "headers": { "cache-control": "public,max-age=0,must-revalidate" }
    }
  ],
  "builds": [
    {
      "src": "build.sh",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],

  "public": true
}
